{
  "name": "railroad/railcontent",
  "type": "library",
  "description": "Very simple and flexible CMS system for developers.",
  "keywords": [
    "cms",
    "developer",
    "railcontent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/laravel-apidoc-generator"
    },
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/usora"
    }
  ],
  "require": {
    "php": "~7.1",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "ramsey/uuid": "^3.6",
    "aws/aws-sdk-php":"^3.0.0",
    "league/flysystem": "^1.0.40",
    "league/flysystem-aws-s3-v3": "~1.0",
    "vimeo/vimeo-api": "^1.2",
    "predis/predis": "^1.1",
    "google/apiclient":"^2.2",
    "railroad/resora": ">=1.0.10 <1.1",
    "railroad/permissions": "^1.0",
    "railroad/usora": "dev-v2.0-skill as 2.0.68",
    "ext-redis": "*",
    "railroad/doctrine": "^0.0",
    "railroad/doctrine-array-hydrator": "^1.0",
    "jms/serializer": "1.*",
    "ruflin/elastica":"7.*",
    "doctrine/search":"dev-master"
  },
  "require-dev": {
    "mpociot/laravel-apidoc-generator": "dev-master",
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "~6.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.6",
    "filp/whoops":"~2.1"
  },
  "autoload": {
    "classmap": [
      "database/seeds"
    ],
    "psr-4": {
      "Railroad\\Railcontent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railcontent\\Tests\\": "tests/"
    }
  }
}
