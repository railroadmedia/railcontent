{
  "name": "railroad/railcontent",
  "type": "library",
  "description": "Very simple and flexible CMS system for developers.",
  "keywords": [
    "cms",
    "developer",
    "railcontent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "doctrine/dbal": "^3.3",
    "ramsey/uuid": "^4.3",
    "aws/aws-sdk-php":"^3.0.0",
    "league/flysystem": "^3.0",
    "league/flysystem-aws-s3-v3": "^3.0",
    "vimeo/vimeo-api": "^1.2",
    "predis/predis": "^1.1",
    "google/apiclient":"^2.2",
    "railroad/response": "2.0.x-dev",
    "elasticsearch/elasticsearch": "~7.0",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds"
    ],
    "psr-4": {
      "Railroad\\Railcontent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railcontent\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": false
    }
  }
}
