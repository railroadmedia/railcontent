{
  "name": "railroad/railcontent",
  "type": "library",
  "description": "Very simple and flexible CMS system for developers.",
  "keywords": [
    "cms",
    "developer",
    "railcontent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/laravel-apidoc-generator"
    },
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/usora"
    }
  ],
  "require": {
    "php": "~8.0",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.4",
    "doctrine/dbal": "^2.5",
    "ramsey/uuid": "^4.2",
    "aws/aws-sdk-php":"^3.0.0",
    "league/flysystem": "^3.0",
    "league/flysystem-aws-s3-v3": "^3.0",
    "vimeo/vimeo-api": "^1.2",
    "predis/predis": "^1.1",
    "google/apiclient":"^2.2",
    "ext-redis": "*",

    "railroad/doctrine-array-hydrator": "^1.0",
    "jms/serializer": "1.*",
    "elasticsearch/elasticsearch": "~7.0",
    "doctrine/search":"dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpunit/php-code-coverage": "~9.2",
    "orchestra/testbench": "~7.0",
    "filp/whoops":"~2.14",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Railcontent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railcontent\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}
